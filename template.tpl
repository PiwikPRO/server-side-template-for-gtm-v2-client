    assertApi('setResponseBody').wasCalledWith('body');
    assertApi('getRequestHeader').wasCalledWith('origin');
    assertApi('setResponseHeader').wasCalledWith(
      'Access-Control-Allow-Origin',
      'origin'
    );
    assertApi('setResponseHeader').wasCalledWith(
      'Access-Control-Allow-Credentials',
      'true'
